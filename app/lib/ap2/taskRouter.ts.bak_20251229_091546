export type AP2Result = { ok: boolean; error?: string; [key: string]: any };

// Canonical AP2 task router (compile-safe stub).
// Replace this with real routing logic once build is stable.
export async function routeTask(type: string, body: any): Promise<AP2Result> {
  return {
    ok: false,
    error: "taskRouter not implemented yet",
    type,
    received: body
  };
}
