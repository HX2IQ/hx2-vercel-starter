
{
  "version": 2,
  "framework": "nextjs",
  "routes": [
    {
      "src": "/src/app/api/(.*)",
      "dest": "/src/app/api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://optinodeiq.com",
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type,Authorization,X-HX2-Internal"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "LOG_LEVEL": "warn",
    "ASSIST_PERSIST": "false"
  }
}
